CREATE DATABASE registration_db;

USE registration_db;

CREATE TABLE users (
  id INT AUTO_INCREMENT PRIMARY KEY,
  name VARCHAR(100),
  age INT,
  dob DATE,
  gender VARCHAR(10),
  mobile VARCHAR(15),
  email VARCHAR(100),
  address TEXT,
  state VARCHAR(50),
  pincode VARCHAR(10),
  occupation VARCHAR(100),
  marital_status VARCHAR(20),
  photo VARCHAR(255)
);
